.PHONY: clean All

All:
	@echo "----------Building project:[ CodingExercise - Debug ]----------"
	@cd "CodingExercise" && "$(MAKE)" -f  "CodingExercise.mk"
clean:
	@echo "----------Cleaning project:[ CodingExercise - Debug ]----------"
	@cd "CodingExercise" && "$(MAKE)" -f  "CodingExercise.mk" clean
