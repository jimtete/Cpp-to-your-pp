.PHONY: clean All

All:
	@echo "----------Building project:[ Nested_If_Statements - Debug ]----------"
	@cd "Nested_If_Statements" && "$(MAKE)" -f  "Nested_If_Statements.mk"
clean:
	@echo "----------Cleaning project:[ Nested_If_Statements - Debug ]----------"
	@cd "Nested_If_Statements" && "$(MAKE)" -f  "Nested_If_Statements.mk" clean
